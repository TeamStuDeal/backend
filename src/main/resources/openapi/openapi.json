    }
  ],
  "paths": {
    "/boards/{boardId}": {
      "get": {
        "tags": [
          "게시판 API"
        ],
        "summary": "게시글 상세 조회 - 선생님",
        "description": "선생님이 조회하는 게시글 상세 정보 API. 게시글 정보와 입찰 순위 정보를 함께 제공합니다.",
        "operationId": "getTeacherDetail",
        "parameters": [
          {
            "name": "boardId",
            "in": "path",
            "description": "게시글 ID",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "responses": {
          "400": {
            "description": "게시글을 찾을 수 없습니다.",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponse"
                }
              }
            }
          },
          "200": {
            "description": "OK, 성공",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponseDetailTeacherResponse"
                }
              }
            }
          }
        }
      },
      "put": {
        "tags": [
          "게시판 API"
        ],
        "summary": "게시글 수정",
        "description": "과외 모집 게시글을 수정합니다. 본인이 작성한 게시글만 수정 가능합니다.",
        "operationId": "updateBoard",
        "parameters": [
          {
            "name": "boardId",
            "in": "path",
            "description": "게시글 ID",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/BoardUpdateRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK, 성공",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponseBoardDetailResponse"
                }
              }
            }
          },
          "400": {
            "description": "게시글 수정 권한이 없습니다.",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponse"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": [
          "게시판 API"
        ],
        "summary": "게시글 삭제",
        "description": "선생님이 작성한 과외 모집 게시글을 삭제합니다.",
        "operationId": "deleteBoard",
        "parameters": [
          {
            "name": "boardId",
            "in": "path",
            "description": "게시글 ID",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK, 성공",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponseJava.lang.Object"
                }
              }
            }
          },
          "400": {
            "description": "게시글 작성자만 삭제할 수 있습니다.",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponse"
                }
              }
            }
          }
        }
      }
    },
    "/negotiations": {
      "post": {
        "tags": [
          "가격 제안(협상)"
        ],
        "summary": "학생 가격 제안(협상) 생성 API",
        "description": "학생이 강사에게 가격 제안을 생성하는 API입니다.",
        "operationId": "createNegotiation",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/com.studeal.team.domain.negotiation.dto.NegotiationRequestDTO$CreateRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK, 성공",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.domain.negotiation.dto.NegotiationResponseDTO"
                }
              }
            }
          },
          "404": {
            "description": "존재하지 않는 학생 또는 강사입니다.",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponse"
                }
              }
            }
          },
          "400": {
            "description": "유효하지 않은 가격 제안 요청입니다.",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponse"
                }
              }
            }
          }
        }
      }
    },
    "/enrollments": {
      "post": {
        "tags": [
          "수업 참여 확정"
        ],
        "summary": "최고가를 부른 학생과 선생님을 연결하는 API",
        "description": "경매 종료 후, 최고가를 부른 학생과 선생님을 연결하는 API입니다.이를 통해 학생은 낙찰받은 수업을 결제하여 거래를 성사하거나, 거절하여 최종 낙찰을 포기할 수 있습니다.",
        "operationId": "createEnrollment",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/com.studeal.team.domain.enrollment.dto.EnrollmentRequestDTO$CreateRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "400": {
            "description": "이미 Enrollment(학생의 수업 참여 확정 현황)가 존재합니다.",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponse"
                }
              }
            }
          },
          "404": {
            "description": "존재하지 않는 협상입니다.",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponse"
                }
              }
            }
          }
        }
      }
    },
    "/boards": {
      "post": {
        "tags": [
          "게시판 API"
        ],
        "summary": "게시글 생성",
        "description": "선생님이 과외 모집 게시글을 작성합니다.",
        "operationId": "createBoard",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/com.studeal.team.domain.board.dto.BoardRequestDTO$CreateRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK, 성공",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponseBoardDetailResponse"
                }
              }
            }
          },
          "400": {
            "description": "선생님을 찾을 수 없습니다.",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponse"
                }
              }
            }
          }
        }
      }
    },
    "/auth/signup": {
      "post": {
        "tags": [
          "사용자 인증"
        ],
        "summary": "회원가입 API",
        "description": "사용자 회원가입을 처리하는 API입니다. 학생/강사 역할에 따라 처리됩니다.",
        "operationId": "signupUser",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/com.studeal.team.domain.user.dto.UserRequestDTO$SignupRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "400": {
            "description": "이미 등록된 이메일입니다.",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponse"
                }
              }
            }
          },
          "200": {
            "description": "OK, 성공",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponseCom.studeal.team.domain.user.dto.UserResponseDTO"
                }
              }
            }
          }
        }
      }
    },
    "/auth/logout": {
      "post": {
        "tags": [
          "사용자 인증"
        ],
        "summary": "로그아웃 API",
        "description": "현재 인증된 사용자를 로그아웃 처리합니다.",
        "operationId": "logout",
        "responses": {
          "200": {
            "description": "OK, 성공",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponseJava.lang.String"
                }
              }
            }
          }
        }
      }
    },
    "/auth/login": {
      "post": {
        "tags": [
          "사용자 인증"
        ],
        "summary": "로그인 API",
        "description": "이메일과 비밀번호로 로그인하여 JWT 토큰을 발급받습니다.",
        "operationId": "login",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK, 성공",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponseCom.studeal.team.domain.user.dto.TokenDTO"
                }
              }
            }
          },
          "400": {
            "description": "잘못된 인증 정보입니다.",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponse"
                }
              }
            }
          }
        }
      }
    },
    "/enrollments/{enrollmentId}/status": {
      "patch": {
        "tags": [
          "수업 참여 확정"
        ],
        "summary": "수업 참여 확정 변경 API",
        "description": "수업 참여 확정 상태를 변경하는 API입니다. (WAITING → CONFIRMED/CANCELED, CONFIRMED → CANCELED 변경 가능) 이 API를 통해 학생은 수업 참여 확정 상태를 업데이트할 수 있습니다.",
        "operationId": "updateStatus",
        "parameters": [
          {
            "name": "enrollmentId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/com.studeal.team.domain.enrollment.dto.EnrollmentRequestDTO$StatusUpdateRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "400": {
            "description": "유효하지 않은 상태 변경 요청입니다.",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponse"
                }
              }
            }
          },
          "404": {
            "description": "존재하지 않는 수강 신청입니다.",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponse"
                }
              }
            }
          }
        }
      }
    },
    "/negotiations/{negotiationId}": {
      "delete": {
        "tags": [
          "가격 제안(협상)"
        ],
        "summary": "학생 가격 제안(협상) 삭제 API",
        "description": "협상 ID로 학생의 가격 제안(협상)을 삭제하는 API입니다.",
        "operationId": "deleteNegotiation",
        "parameters": [
          {
            "name": "negotiationId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK, 성공",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponseJava.lang.Void"
                }
              }
            }
          },
          "404": {
            "description": "존재하지 않는 협상입니다.",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/com.studeal.team.global.error.ApiResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "BoardUpdateRequest": {
        "required": [
          "expectedPrice",
          "major",
          "subject",
          "title"
        ],
        "type": "object",
        "properties": {
          "title": {
            "maxLength": 200,
            "minLength": 0,
            "type": "string",
            "description": "게시글 제목",
            "example": "고등 수학 과외 모집합니다 (수정)"
          },
          "content": {
            "maxLength": 2000,
            "minLength": 0,
            "type": "string",
            "description": "게시글 내용",
            "example": "수학 전공 대학생이 수학 과외를 모집합니다. 문제 풀이 및 개념 설명에 능숙합니다. (내용 수정)"
          },
          "major": {
            "type": "string",
            "description": "게시글 유형",
            "example": "MATH",
            "enum": [
              "수학",
              "과학",
              "영어",
              "국어",
              "역사",
              "코딩"
            ]
          },
          "expectedPrice": {
            "type": "integer",
            "description": "예상 수업 가격",
            "format": "int64",
            "example": 55000
          },
          "subject": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string",
            "description": "수업 주제",
            "example": "고등 수학 - 미적분과 통계"
          }
        },
        "description": "게시글 수정 요청"
      },
      "com.studeal.team.global.error.ApiResponse": {
        "type": "object",
        "properties": {
          "isSuccess": {
            "type": "boolean",
            "description": "성공 여부",
            "example": true
          },
          "httpStatusCode": {
            "type": "integer",
            "description": "HTTP 상태 코드",
            "format": "int32",
            "example": 200
          },
          "code": {
            "type": "string",
            "description": "커스텀 응답 코드",
            "example": "COMMON200"
          },
          "message": {
            "type": "string",
            "description": "응답 메시지",
            "example": "성공입니다."
          },
          "result": {
            "type": "object",
            "description": "응답 데이터"
          }
        },
        "description": "API 응답 형식"
      },
      "BoardDetailResponse": {
        "type": "object",
        "properties": {
          "boardId": {
            "type": "integer",
            "description": "게시글 ID",
            "format": "int64",
            "example": 1
          },
          "title": {
            "type": "string",
            "description": "게시글 제목",
            "example": "고등 수학 과외 모집합니다"
          },
          "content": {
            "type": "string",
            "description": "게시글 내용",
            "example": "수학 전공 대학생이 수학 과외를 모집합니다. 문제 풀이 및 개념 설명에 능숙합니다."
          },
          "teacherId": {
            "type": "integer",
            "description": "선생님 ID",
            "format": "int64",
            "example": 1
          },
          "teacherName": {
            "type": "string",
            "description": "선생님 이름",
            "example": "김선생"
          },
          "major": {
            "type": "string",
            "description": "게시글 유형",
            "example": "MATH",
            "enum": [
              "수학",
              "과학",
              "영어",
              "국어",
              "역사",
              "코딩"
            ]
          },
          "expectedPrice": {
            "type": "integer",
            "description": "예상 수업 가격",
            "format": "int64",
            "example": 50000
          },
          "specMajor": {
            "type": "string",
            "description": "구체적인 과외 주제",
            "example": "고등 수학 - 미적분"
          },
          "createdAt": {
            "type": "string",
            "description": "생성 일시",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "description": "수정 일시",
            "format": "date-time"
          },
          "thumbnailUrl": {
            "type": "string",
            "description": "썸네일 이미지 URL",
            "example": "/images/thumbnails/math-123.jpg"
          }
        },
        "description": "게시글 상세 응답"
      },
      "com.studeal.team.global.error.ApiResponseBoardDetailResponse": {
        "type": "object",
        "properties": {
          "isSuccess": {
            "type": "boolean",
            "description": "성공 여부",
            "example": true
          },
          "httpStatusCode": {
            "type": "integer",
            "description": "HTTP 상태 코드",
            "format": "int32",
            "example": 200
          },
          "code": {
            "type": "string",
            "description": "커스텀 응답 코드",
            "example": "COMMON200"
          },
          "message": {
            "type": "string",
            "description": "응답 메시지",
            "example": "성공입니다."
          },
          "result": {
            "$ref": "#/components/schemas/BoardDetailResponse"
          }
        },
        "description": "API 응답 형식"
      },
      "com.studeal.team.domain.negotiation.dto.NegotiationRequestDTO$CreateRequest": {
        "required": [
          "proposedPrice",
          "studentId",
          "teacherId"
        ],
        "type": "object",
        "properties": {
          "studentId": {
            "type": "integer",
            "format": "int64"
          },
          "teacherId": {
            "type": "integer",
            "format": "int64"
          },
          "proposedPrice": {
            "type": "integer",
            "format": "int64"
          }
        }
      },
      "com.studeal.team.domain.negotiation.dto.NegotiationResponseDTO": {
        "type": "object",
        "properties": {
          "negotiationId": {
            "type": "integer",
            "format": "int64"
          },
          "studentId": {
            "type": "integer",
            "format": "int64"
          },
          "teacherId": {
            "type": "integer",
            "format": "int64"
          },
          "proposedPrice": {
            "type": "integer",
            "format": "int64"
          },
          "status": {
            "type": "string",
            "enum": [
              "PENDING",
              "ACCEPTED",
              "REJECTED"
            ]
          }
        }
      },
      "com.studeal.team.domain.enrollment.dto.EnrollmentRequestDTO$CreateRequest": {
        "required": [
          "negotiationId",
          "studentId"
        ],
        "type": "object",
        "properties": {
          "negotiationId": {
            "type": "integer",
            "format": "int64"
          },
          "studentId": {
            "type": "integer",
            "format": "int64"
          }
        }
      },
      "com.studeal.team.domain.board.dto.BoardRequestDTO$CreateRequest": {
